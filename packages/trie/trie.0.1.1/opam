build: [
  [make]
  [make "doc"] {with-doc}
]
install: [
  [make "install"]
  [make "DOCDIR=%{doc}%/trie" "install-doc"] {with-doc}
]
remove: [
  ["ocamlfind" "remove" "trie"]
  ["rm" "-rf" "%{doc}%/trie"]
]
