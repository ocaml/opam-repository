opam-version: "1"
maintainer: "sebastien.fricker@gmail.com"
build: [
  [ "qmake" "CONFIG-=app_bundle" "CONFIG+=release" "PREFIX_BIN=%{bin}%/" "CONFIG-=debug" "oqamldebug.pro" ]
  ["%{make}%"]
  ["%{make}%" "install" ]
]
remove: [
  [ "qmake" "CONFIG-=app_bundle" "CONFIG+=release" "PREFIX_BIN=%{bin}%/" "CONFIG-=debug" "oqamldebug.pro" ]
  [ "make" "uninstall" ]
]

