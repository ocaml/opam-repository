  "ocamlbuild"
  "-use-ocamlfind"
  "-plugin-tag"
  "package(cppo_ocamlbuild)"
  "General.cma"
  "General.cmxa"
]
install: [
  ["mkdir" "-p" "%{prefix}%/lib/General"]
  [
    "cp"
    "META"
    "_build/src/General.cmi"
    "_build/src/General.cma"
    "_build/src/General.a"
    "_build/src/General.cmxa"
    "%{prefix}%/lib/General"
  ]
]
remove: ["ocamlfind" "remove" "General"]
depends: [
  "ocamlbuild" {build}
  "ocamlfind" {build}
  "cppo" {build & >= "1.3" & < "2"}
]
available: [ocaml-version >= "4.02.2" & ocaml-version < "5"]
